{
  "name": "openbiocard-backend",
  "version": "1.0.0",
  "description": "OpenBioCard Backend",
  "main": "index.tsx",
  "scripts": {
    "dev": "wrangler dev -c ../../wrangler.jsonc",
    "build": "pnpm --filter openbiocard-frontend build && rm -rf dist && cp -r ../frontend/dist ./dist && wrangler deploy --dry-run -c ../../wrangler.jsonc",
    "deploy": "wrangler deploy -c ../../wrangler.jsonc",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings -c ../../wrangler.jsonc"
  },
  "dependencies": {
    "hono": "^4.10.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "typescript": "^5.0.0",
    "wrangler": "^4.55.0"
  }
}